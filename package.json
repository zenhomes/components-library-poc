{
    "name": "@zenhomes/react-shared",
    "version": "12.10.1",
    "description": "Reusable custom react components & style utils",
    "author": "toystars & buddies",
    "license": "MIT",
    "repository": "toystars/react-shared",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
        "test:watch": "react-scripts-ts test --env=jsdom",
        "build": "rollup -c",
        "start": "rollup -c -w",
        "predeploy": "cd example && npm install && npm run build",
        "deploy": "gh-pages -d example/build",
        "update-colors": "npm i @zenhomes/ui@latest --save-dev && ./src/scripts/update-color-variables.sh",
        "storybook": "start-storybook",
        "build-storybook": "build-storybook",
        "init": "zenbit init"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "@tippy.js/react": "^3.1.1",
        "@types/lodash": "^4.14.138",
        "@zenhomes/domain": "^2.5.0",
        "@zenhomes/sauron": "^0.7.0",
        "@zenhomes/ui": "^6.6.1",
        "accounting": "^0.4.1",
        "classnames": "^2.2.6",
        "isomorphic-unfetch": "^3.0.0",
        "lodash": "^4.17.15",
        "ramda": "^0.26.1",
        "react-calendar": "^2.19.2",
        "react-dnd": "^10.0.2",
        "react-dnd-html5-backend": "^10.0.2",
        "react-icons": "^3.7.0",
        "react-load-script": "0.0.6",
        "react-pdf": "^4.1.0",
        "smooth-scroll-into-view-if-needed": "^1.1.23",
        "zenbit": "file:../zenbit"
    },
    "peerDependencies": {
        "prop-types": "^15.5.4",
        "react": "^15.0.0 || ^16.0.0",
        "react-dom": "^15.0.0 || ^16.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@rollup/plugin-url": "^4.0.2",
        "@storybook/addon-a11y": "^5.2.6",
        "@storybook/addon-actions": "^5.2.6",
        "@storybook/addon-info": "^5.3.17",
        "@storybook/addon-links": "^5.2.6",
        "@storybook/addon-viewport": "^5.2.6",
        "@storybook/addons": "^5.2.6",
        "@storybook/react": "^5.2.6",
        "@svgr/rollup": "^5.2.0",
        "@types/accounting": "^0.4.1",
        "@types/classnames": "^2.2.9",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/googlemaps": "^3.38.1",
        "@types/jest": "^22.2.3",
        "@types/react": "^16.3.13",
        "@types/react-dom": "^16.0.5",
        "@types/react-pdf": "^4.0.0",
        "@types/styled-jsx": "^2.2.8",
        "@zenhomes/icons": "^5.1.1",
        "babel-core": "^6.26.3",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.0.6",
        "babel-runtime": "^6.26.0",
        "bootstrap-scss": "^4.4.0",
        "cross-env": "^5.1.4",
        "css-loader": "^3.2.0",
        "date-fns": "^2.4.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "gh-pages": "^1.2.0",
        "husky": "^1.1.4",
        "jest": "^23.6.0",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^4.13.1",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.0",
        "react": "^16.8.6",
        "react-docgen-typescript-loader": "^3.4.0",
        "react-dom": "^16.8.6",
        "react-scripts-ts": "^2.16.0",
        "rollup": "^1.32.0",
        "rollup-plugin-babel": "^3.0.7",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-postcss": "^2.2.0",
        "rollup-plugin-typescript2": "^0.26.0",
        "sass-loader": "^8.0.0",
        "scss-to-json": "^2.0.0",
        "style-loader": "^1.0.0",
        "ts-jest": "^22.4.6",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.3",
        "webpack": "^4.41.2"
    },
    "files": [
        "dist"
    ],
    "bit": {
        "env": {
            "compiler": "bit.envs/compilers/react@1.0.16"
        },
        "componentsDefaultDirectory": "src/components/{name}",
        "packageManager": "npm"
    }
}
